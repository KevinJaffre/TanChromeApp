{
  "manifest_version":2,
  "name":"Tan",
  "version": "1.0.0",
  "description":"Horaires de bus",
  "permissions": [
    "http://*.tan.fr/",
    "notifications",
    "alarms"
  ],
  "icons": {
    "128": "img/icon.png"
  },
  "background": {
    "scripts": ["js/jquery-2.2.3.min.js","background.js"]
  },
  "browser_action": {
          "default_icon": {
            "19": "img/icon.png",
            "38": "img/icon.png"
          },
          "default_title": "Prends ton bus !",
          "default_popup": "index.html"
        }
}
